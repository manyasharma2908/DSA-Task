class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
       int st=0;
       int end=letters.length-1;
       char ans=letters[0];
       while(st<=end){
        int mid=st+(end-st)/2;
        if(letters[mid]<=target){
            st=mid+1;
        }
        else {
            end=mid-1;
            ans=letters[mid];
        }
       
       }
      return ans;
    }
}
